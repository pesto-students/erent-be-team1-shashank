{
  "name": "erent-be",
  "version": "1.0.0",
  "description": "erent backend server",
  "main": "server.js",
  "scripts": {
    "start": "node dist/server.js",
    "prestart": "npm run build",
    "dev": "env-cmd -f ./envs/dev.env nodemon --exec babel-node src/server.js",
    "build": "npm run clean && babel src --out-dir dist --copy-files",
    "clean": "rm -rf dist",
    "lint": "eslint src/*",
    "lint:fix": "eslint --fix src/*",
    "prepare": "husky install",
    "postinstall": "husky install",
    "preinstall": "chmod a+x .husky/commit-msg",
    "commit": "cz"
  },
  "keywords": [
    "erent",
    "nodejs",
    "expressjs",
    "mongodb"
  ],
  "author": "team shashank",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "7.18.10",
    "@babel/core": "7.18.10",
    "@babel/node": "7.18.10",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/preset-env": "7.18.10",
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "babel-eslint": "10.1.0",
    "babel-plugin-module-resolver": "4.1.0",
    "commitizen": "4.2.5",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "16.0.1",
    "eslint": "8.21.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.26.0",
    "husky": "^8.0.0",
    "lint-staged": "13.0.3",
    "morgan": "1.10.0",
    "nodemon": "2.0.19",
    "prettier": "2.7.1"
  },
  "dependencies": {
    "@magic-sdk/admin": "1.4.1",
    "bcryptjs": "2.4.3",
    "cloudinary": "1.30.1",
    "colors": "1.4.0",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dayjs": "1.11.4",
    "env-cmd": "10.1.0",
    "express": "4.18.1",
    "express-fileupload": "1.4.0",
    "express-mongo-sanitize": "2.2.0",
    "express-rate-limit": "6.5.1",
    "helmet": "5.1.1",
    "hpp": "0.2.3",
    "http-status": "1.5.2",
    "joi": "17.6.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "mongoose": "6.5.0",
    "nodemailer": "6.7.7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{css,less,scss,html,json,jsx,js}": [
      "prettier --write ."
    ],
    "*.js": "eslint --fix"
  }
}
